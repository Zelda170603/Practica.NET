<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - dotnetproyect</title>
    <script src="https://cdn.tailwindcss.com"></script>
</head>
<body class="bg-gray-800 mx-auto ">
    
        <nav class="bg-white border-gray-200 px-4 lg:px-6 py-2.5 dark:bg-gray-800">
            <div class="flex flex-wrap justify-between items-center mx-auto max-w-screen-xl">
                <a href="" class="flex items-center">
                    <img src="https://seeklogo.com/images/U/unan-logo-9E327775E3-seeklogo.com.png" class="mr-3 h-8 sm:h-9" alt="Flowbite Logo" />
                    <span class="self-center text-xl font-semibold whitespace-nowrap dark:text-white">Unan Leon</span>
                </a>
            <partial name="_LoginPartial"/>
            </div>
        </nav>
    <main class="container mx-auto p-4 min-h-full mt-18">
        @RenderBody()
    </main>
    
    
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
